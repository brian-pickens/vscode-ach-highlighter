      "RecordTypeCode": {
        "StartPosition": 1,
        "EndPosition": 1,
        "Description": "Always 5"
      },
      "ServiceClassCode": {
        "StartPosition": 2,
        "EndPosition": 4,
        "Description": "Service class code"
      },
      "CompanyName": {
        "StartPosition": 5,
        "EndPosition": 20,
        "Description": "Company name"
      },
      "CompanyDiscretionaryData": {
        "StartPosition": 21,
        "EndPosition": 40,
        "Description": "Discretionary data"
      },
      "CompanyIdentification": {
        "StartPosition": 41,
        "EndPosition": 50,
        "Description": "Company ID"
      },
      "StandardEntryClassCode": {
        "StartPosition": 51,
        "EndPosition": 53,
        "Description": "SEC Code (e.g., PPD, CCD)"
      },
      "CompanyEntryDescription": {
        "StartPosition": 54,
        "EndPosition": 63,
        "Description": "Entry description"
      },
      "CompanyDescriptiveDate": {
        "StartPosition": 64,
        "EndPosition": 69,
        "Description": "Descriptive date"
      },
      "EffectiveEntryDate": {
        "StartPosition": 70,
        "EndPosition": 75,
        "Description": "Effective date"
      },
      "OriginatorStatusCode": {
        "StartPosition": 76,
        "EndPosition": 76,
        "Description": "Always 1"
      },
      "OriginatingDFIIdentification": {
        "StartPosition": 77,
        "EndPosition": 84,
        "Description": "Originating DFI ID"
      },
      "BatchNumber": {
        "StartPosition": 85,
        "EndPosition": 94,
        "Description": "Batch number"
      }
    }
  },
  "EntryDetailRecord": {
    "TypeCode": 6,
    "Description": "Contains individual entry details like account number, amount, and transaction code.",
    "Fields": {
      "RecordTypeCode": {
        "StartPosition": 1,
        "EndPosition": 1,
        "Description": "Always 6"
      },
      "TransactionCode": {
        "StartPosition": 2,
        "EndPosition": 3,
        "Description": "Transaction type code"
      },
      "ReceivingDFIIdentification": {
        "StartPosition": 4,
        "EndPosition": 11,
