  "BatchControlRecord": {
    "TypeCode": 8,
    "Description": "Summarizes the batch, providing totals for debits, credits, and counts.",
    "Fields": {
      "RecordTypeCode": {
        "StartPosition": 1,
        "EndPosition": 1,
        "Description": "Always 8"
      },
      "ServiceClassCode": {
        "StartPosition": 2,
        "EndPosition": 4,
        "Description": "Same as batch header"
      },
      "EntryAddendaCount": {
        "StartPosition": 5,
        "EndPosition": 10,
        "Description": "Number of entries"
      },
      "EntryHash": {
        "StartPosition": 11,
        "EndPosition": 20,
        "Description": "Hash of routing numbers"
      },
      "TotalDebitEntryDollarAmount": {
        "StartPosition": 21,
        "EndPosition": 32,
        "Description": "Total debits"
      },
      "TotalCreditEntryDollarAmount": {
        "StartPosition": 33,
        "EndPosition": 44,
        "Description": "Total credits"
      },
      "CompanyIdentification": {
        "StartPosition": 45,
        "EndPosition": 54,
        "Description": "Same as batch header"
      },
      "OriginatingDFIIdentification": {
        "StartPosition": 55,
        "EndPosition": 62,
        "Description": "Originating DFI ID"
      },
      "BatchNumber": {
        "StartPosition": 63,
        "EndPosition": 73,
        "Description": "Batch number"
      }
    }
  },
  "FileControlRecord": {
    "TypeCode": "9",
    "Description": "Summarizes the entire file, including batch count, entry count, and dollar totals.",
