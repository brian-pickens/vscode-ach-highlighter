      },
      "Amount": {
        "StartPosition": 30,
        "EndPosition": 39,
        "Description": "Transaction amount"
      },
      "IndividualIdentificationNumber": {
        "StartPosition": 40,
        "EndPosition": 54,
        "Description": "Identification number"
      },
      "IndividualName": {
        "StartPosition": 55,
        "EndPosition": 76,
        "Description": "Receiver's name"
      },
      "DiscretionaryData": {
        "StartPosition": 77,
        "EndPosition": 78,
        "Description": "Optional data"
      },
      "AddendaRecordIndicator": {
        "StartPosition": 79,
        "EndPosition": 79,
