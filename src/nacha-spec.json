{
  "FileHeaderRecord": {
    "TypeCode": 1,
    "Description": "Identifies the file's sender, receiver, and creation details.",
    "Fields": {
      "RecordTypeCode": {
        "StartPosition": 1,
        "EndPosition": 1,
        "Description": "Always 1"
      },
      "PriorityCode": {
        "StartPosition": 2,
        "EndPosition": 3,
        "Description": "Priority code (01)"
      },
      "ImmediateDestination": {
        "StartPosition": 4,
        "EndPosition": 13,
        "Description": "Routing number of destination"
      },
      "ImmediateOrigin": {
        "StartPosition": 14,
        "EndPosition": 23,
        "Description": "Routing number of origin"
      },
      "FileCreationDate": {
        "StartPosition": 24,
        "EndPosition": 29,
        "Description": "Creation date (YYMMDD)"
      },
      "FileCreationTime": {
        "StartPosition": 30,
        "EndPosition": 33,
        "Description": "Creation time (HHMM)"
      },
      "FileIDModifier": {
        "StartPosition": 34,
        "EndPosition": 34,
        "Description": "Unique file identifier"
      },
      "RecordSize": {
        "StartPosition": 35,
        "EndPosition": 37,
        "Description": "Always 094"
      },
      "BlockingFactor": {
        "StartPosition": 38,
        "EndPosition": 39,
        "Description": "Always 10"
      },
      "FormatCode": {
        "StartPosition": 40,
        "EndPosition": 40,
        "Description": "Always 1"
      },
      "ImmediateDestinationName": {
        "StartPosition": 41,
        "EndPosition": 63,
        "Description": "Name of immediate destination"
      },
      "ImmediateOriginName": {
        "StartPosition": 64,
        "EndPosition": 86,
        "Description": "Name of immediate origin"
      },
      "ReferenceCode": {
        "StartPosition": 87,
        "EndPosition": 94,
        "Description": "Reference code"
      }
    }
  },
  "BatchHeaderRecord": {
    "TypeCode": 5,
    "Description": "Defines details of a specific batch, including company and service class.",
