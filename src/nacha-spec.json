      },
      "EntryAddendaCount": {
        "StartPosition": 13,
        "EndPosition": 18,
        "Description": "Total number of entries"
      },
      "EntryHash": {
        "StartPosition": 19,
        "EndPosition": 28,
        "Description": "Hash of routing numbers"
      },
      "TotalDebitEntryDollarAmount": {
        "StartPosition": 29,
        "EndPosition": 40,
        "Description": "Total debits"
      },
      "TotalCreditEntryDollarAmount": {
        "StartPosition": 41,
        "EndPosition": 52,
        "Description": "Total credits"
      }
    }
  }
}
